{
    "name": "osteel/duct",
    "description": "File processor written in PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Osteel\\Duct\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osteel\\Duct\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Yannick Chenot",
            "email": "yannick@yellowraincoat.co.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "symfony/console": "^6.0",
        "illuminate/collections": "^9.6",
        "symfony/yaml": "^6.0",
        "vlucas/phpdotenv": "^5.4",
        "illuminate/support": "^9.6"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "bin": [
        "bin/duct"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
